{
  "name": "swagger-typescript-client-generator",
  "version": "1.1.0",
  "description": "Tool to convert swagger 2.0 file to typescript client",
  "main": "lib/index.js",
  "bin": "bin/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/psrebniak/swagger-typescript-client-generator.git"
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "prebuild": "npm run clean && npm run test",
    "build": "tsc -p .",
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "prepublish": "npm run test && npm run build",
    "lint": "tslint --project . --format verbose"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^1.6.0",
    "prettier": "^1.16.4",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "8",
    "@types/prettier": "^1.16.1",
    "@types/swagger-schema-official": "^2.0.15",
    "@types/yargs": "^12.0.11",
    "mocha": "^6.0.2",
    "rimraf": "^2.6.3",
    "swagger-schema-official": "latest",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.4.1"
  },
  "keywords": [
    "swagger",
    "typescript",
    "client"
  ]
}
